{
  "compilerOptions": {
    "target": "ES6",
    "module": "none",
    "outDir": "./js",
    "rootDir": "./ts",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES6", "DOM"],
    "types": ["jquery", "jqueryui"]
  },
  "include": ["ts/**/*.ts"]
}
